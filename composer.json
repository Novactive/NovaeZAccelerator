{
    "name": "novactive/ezaccelerator",
    "description": "Novactive eZ Accelerator is an eZ Platform bundle for performance optimizations.",
    "keywords": [
        "performance",
        "ezplatform",
        "novactive",
        "asynchronous",
        "bus",
        "queue"
    ],
    "homepage": "https://github.com/Novactive/NovaeZAccelerator",
    "type": "ezplatform-bundle",
    "authors": [
        {
            "name": "Novactive",
            "homepage": "https://www.novactive.us",
            "email": "dir.tech@novactive.com"
        },
        {
            "name": "Sebastien Morel",
            "homepage": "https://github.com/Plopix",
            "email": "morel.seb@gmail.com"
        }
    ],
    "license": [
        "MIT"
    ],
    "require": {
        "php": "^7.3",
        "ext-dom": "*",
        "ext-pdo": "*",
        "ext-json": "*",
        "ezsystems/ezplatform-kernel": "~1.1.0",
        "symfony/messenger": "*"
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "^3.4",
        "phpmd/phpmd": "^2.7",
        "friendsofphp/php-cs-fixer": "^2.15",
        "phpunit/phpunit": "^8.0",
        "symfony/panther": "^0.7.1"
    },
    "autoload": {
        "psr-4": {
            "Novactive\\Bundle\\eZAccelerator\\": "bundle",
            "Novactive\\Bundle\\eZAccelerator\\Tests\\": "tests/Tests"
        }
    },
    "extra": {
        "thanks": {
            "name": "ezsystems/ezplatform",
            "url": "https://github.com/ezsystems/ezplatform"
        }
    }
}
